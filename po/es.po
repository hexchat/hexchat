#: src/common/outbound.c:1620
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Argumento desconocido «%s» ignorado."

#: src/common/outbound.c:2294
msgid "No such plugin found.\n"
msgstr "No se encontró la extensión (NT:plugin).\n"

#: src/common/outbound.c:2299 src/fe-gtk/plugingui.c:132
msgid "That plugin is refusing to unload.\n"
msgstr "Esta extensión se esta negando a ser removida.\n"

#: src/common/outbound.c:2433
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <nombre> <acción>, agrega un botón debajo de la lista de usuarios"

#: src/common/outbound.c:2435
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr ""
"ALLCHAN <cmd>, envía un comando a todos los canales adonde se encuentra "
"conectado"

#: src/common/outbound.c:2437
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr ""
"ALLSERV <cmd>, envía un comando a todos los servidores adonde se encuentra "
"conectado"

#: src/common/outbound.c:2438
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<razón>], define su ausencia"

#: src/common/outbound.c:2440
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"BAN <mascara> [<tipo de veto>], veta a todos aquellos que concuerden con la "
"mascara del canal actual. Si ya están en el canal esto no los expulsa (NT:"
"kick) (es necesario ser operador del canal)"

#: src/common/outbound.c:2442
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr "UNBAN <marcara> [<mascara>...], reincorpora a las mascaras especificadas."

#: src/common/outbound.c:2443
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, Limpia la ventana de texto actual"

#: src/common/outbound.c:2444
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, Cierra la ventana o pestaña actual"

#: src/common/outbound.c:2447
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"CTCP <apodo> <mensaje>, envía el mensaje CTCP al apodo, los mensajes comunes "
"son VERSION y USERINFO"

#: src/common/outbound.c:2449
msgid "COUNTRY <code>, finds a country code, eg: au = australia"
msgstr "COUNTRY <codigo>, busca un código de país, ej: au = Australia"

#: src/common/outbound.c:2451
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, se va del canal actual y se re-conecta inmediatamente"

#: src/common/outbound.c:2453
msgid ""
"\n"
"DCC GET <nick>                     - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file] - send a file to someone\n"
"DCC LIST                           - show DCC list\n"
"DCC CHAT <nick>                    - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <apodo>                         - recibe un archivo ofrecido\n"
"DCC SEND [-maxcps=#] <apodo> [archivo]  - envía un archivo a una persona\n"
"DCC LIST                                - muestra la lista DCC\n"
"DCC CHAT <apodo>                        - ofrece el DCC CHAT a alguien\n"
"DCC CLOSE <tipo> <apodo> <archivo>          ejemplo:\n"
"         /dcc close send juanperez archivo.tar.gz"

#: src/common/outbound.c:2463
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <nombre>, borra un botón de debajo de la lista de usuarios"

#: src/common/outbound.c:2465
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"DEHOP <apodo>, quita el estado de semi-operador del canal al apodo en el "
"canal actual (necesita ser operador del canal)"

#: src/common/outbound.c:2467
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEOP <apodo>, quita el estado de operador al apodo en el canal actual "
"(necesita ser operador del canal)"

#: src/common/outbound.c:2469
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEVOICE <apodo>, quita el estado de voz del apodo en el canal actual "
"(necesita ser operador del canal)"

#: src/common/outbound.c:2470
msgid "DISCON, Disconnects from server"
msgstr "DISCON, Se desconecta del servidor"

#: src/common/outbound.c:2471
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <apodo|equipo|ip>, Busca la dirección IP de un usuario"

#: src/common/outbound.c:2472
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <texto>, Imprime texto localmente"

#: src/common/outbound.c:2475
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"EXEC [-o] <comando>, ejecuta el comando. Si se usa la bandera -o entonces la "
"salida es enviada al canal actual, sino es impresa en el cuadro de texto actual"

#: src/common/outbound.c:2477
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"EXECKILL [-9], mata un ejecutable corriendo en la sesión actual. Si de "
"indica -9 el proceso es matado por la fuerza"

#: src/common/outbound.c:2479
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, envía al proceso la señal de detenerse"

#: src/common/outbound.c:2480
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, envía al proceso la señal de continuar"

#: src/common/outbound.c:2481
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, envía los datos a la entrada estándar del proceso"

#: src/common/outbound.c:2485
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, borra la cola de envío actual del servidor"

#: src/common/outbound.c:2487
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr ""
"GATE <equipo> [<puerto>], utiliza un proxy a través de un equipo, el puerto "
"predeterminado es el 23"

#: src/common/outbound.c:2492
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr ""
"HOP <apodo>, otorga estado de semi-operador al apodo (necesita ser operador "
"del canal)"

#: src/common/outbound.c:2494
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <mascara> <tipos..> <opciones..>\n"
"    mascara - mascara de equipos a ignorar, ej: *!*@*.aol.com\n"
"    tipos - tipos de datos a ignorar, uno o todos de:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    opciones - NOSAVE, QUIET"

#: src/common/outbound.c:2501
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"INVITE <apodo> [<canal>], invita a alguien a un canal, por omisión el canal "
"actual (necesita ser operador del canal)"

#: src/common/outbound.c:2502
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <canal>, se conecta al canal"

#: src/common/outbound.c:2504
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"KICK <apodo>, expulsa al apodo del canal actual (necesita ser operador del "
"canal)"

#: src/common/outbound.c:2506
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"KICKBAN <apodo>, veta y luego expulsa al apodo del canal actual (necesita "
"ser operador del canal)"

#: src/common/outbound.c:2508
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, fuerza una nueva prueba de retardo"

#: src/common/outbound.c:2510
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <texto>, busca el texto en el buffer"

#: src/common/outbound.c:2512
msgid "LOAD <file>, loads a plugin or script"
msgstr "LOAD <archivo>, carga una extensión (plugin) o un guión (script)"

#: src/common/outbound.c:2515
msgid "MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr ""
"MDEHOP, Quita masivamente el estado de semi-operador en el canal actual "
"(necesita ser operador del canal)"

#: src/common/outbound.c:2517
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr ""
"MDEOP, Quita masivamente el estado de operador en el canal actual (necesita "
"ser operador del canal)"

#: src/common/outbound.c:2519
msgid "MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr ""
"MKICK, Expulsa masivamente a todos excepto a usted en el canal actual "
"(necesita ser operador del canal)"

#: src/common/outbound.c:2521
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"ME <acción>, envía la acción al canal actual (las acciones están escritas en "
"tercera persona del inglés, como /me jumps)"

#: src/common/outbound.c:2522
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <apodo> <mensaje>, envía un mensaje privado"

#: src/common/outbound.c:2525
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, Lista los apodos del canal actual"

#: src/common/outbound.c:2527
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <apodo> <mensaje>, Envía una noticia CTCP"

#: src/common/outbound.c:2528
msgid "NEWSERVER <hostname> [<port>]"
msgstr "NEWSERVER <nombre del equipo> [<puerto>]"

#: src/common/outbound.c:2529
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <apodo>, establece su apodo"

#: src/common/outbound.c:2532
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"NOTICE <apodo/canal> <mensaje>, envía una noticia. Las noticias son una "
"especie de mensaje que debería ser auto publicado"

#: src/common/outbound.c:2534
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr "NOTIFY [<apodo>], lista su lista de notificaciones o agrega a alguien a ella"

#: src/common/outbound.c:2536
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr ""
"OP <apodo>, otorga el estado de operador del canal al apodo (necesita ser "
"operador del canal)"

#: src/common/outbound.c:2538
msgid "PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr "PART [<canal>] [<razón>], abandona el canal, por omisión el canal actual"

#: src/common/outbound.c:2540
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr "PING <apodo | canal>, Envía pings CTCP al apodo o canal"

#: src/common/outbound.c:2542
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr "QUERY <apodo>, abre una nueva ventana privada de mensajes hacia alguien"

#: src/common/outbound.c:2544
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<razón>], se desconecta del servidor actual"

#: src/common/outbound.c:2546
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <texto>, envía el texto en formato plano hacia el servidor"

#: src/common/outbound.c:2549
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"RECONNECT [-ssl] [<equipo>] [<puerto>] [<contraseña>], Puede ser llamado "
"solo como /RECONNECT para re-conectarse al servidor actual o con /"
"RECONNECT_ALL para re-conectarse a todos los servidores abiertos"

#: src/common/outbound.c:2552
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"RECONNECT [<equipo>] [<puerto>] [<contraseña>], Puede ser llamado solo como /"
"RECONNECT para re-conectarse al servidor actual o con /RECONNECT ALL para re-"
"conectarse a todos los servidores abiertos"

#: src/common/outbound.c:2554
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"RECV <texto>, envía datos planos hacia xchat como si estos fuesen recibidos "
"desde el servidor IRC"

#: src/common/outbound.c:2557
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <texto>, envía el texto hacia un objeto de la ventana actual"

#: src/common/outbound.c:2558
msgid "SET <variable> [<value>]"
msgstr "SET <variable> [<valor>]"

#: src/common/outbound.c:2562
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN [-ssl] <equipo> <puerto> <canal>, Lo conecta y une a un canal"

#: src/common/outbound.c:2565
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN <host> <port> <channel>, Lo conecta y une a un canal"

#: src/common/outbound.c:2569
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr ""
"SERVER [-ssl] <equipo> [<puerto>] [<contraseña>], lo conecta al servidor, el "
"puerto predeterminado es 6667 para conexiones normales y 9999 para "
"conexiones ssl"

#: src/common/outbound.c:2572
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr ""
"SERVER <equipo> [<puerto>] [<contraseña>], lo conecta a un servidor, el "
"puerto predeterminado es 6667"

#: src/common/outbound.c:2575
msgid "TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr ""
"TOPIC [<tópico>], Establece el tópico si se provee uno, sino muestra el "
"tópico actual"

#: src/common/outbound.c:2576
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <mascara> [QUIET]"

#: src/common/outbound.c:2577
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "UNLOAD <nombre>, desactiva una extensión (plugin) o un guión (script)"

#: src/common/outbound.c:2580
msgid "WALLCHOP <message>, sends the message to all chanops on the current channel"
msgstr ""
"WALLCHOP <mensaje>, envía el mensaje a todos los operadores del canal en el "
"canal actual"

#: src/common/outbound.c:2582
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <mensaje>, escribe el mensaje a todos los canales"

#: src/common/outbound.c:2584
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr ""
"VOICE <apodo>, le otorga el estado de «voice» a alguien (necesita ser "
"operador del canal)"

