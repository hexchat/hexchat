#: src/common/outbound.c:281
#, c-format
msgid "Server %s already exists on network %s.\n"
msgstr "El servidor %s ya existe en la red %s.\n"

#: src/common/outbound.c:287
#, c-format
msgid "Added server %s to network %s.\n"
