msgid ""
"Userlist Buttons - Special codes:\n"
"\n"
"%a  =  all selected nicks\n"
"%c  =  current channel\n"
"%e  =  current network name\n"
"%h  =  selected nick's hostname\n"
"%m  =  machine info\n"
