#: src/common/outbound.c:2452
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <vardas> <veiksmas> - prideda mygtuka po vartotojø sàraðu"

#: src/common/outbound.c:2454
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr "ALLCHAN <komanda> - nusiunèia komandà á visus kanalus, kuriuose tu esi"

#: src/common/outbound.c:2456
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr ""
"ALLSERV <komanda> - nusiunèia komandà visiems serveriams, prie kuriø tu esi "
"prisijungæs"

#: src/common/outbound.c:2457
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<prieþastis>] - ájungia bûsenà ¥Manæs nëra´"

#: src/common/outbound.c:2459
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"BAN <mask> [<bano tipas>] - iðbanina ið esamo kanalo visus, kuriø maskas "
"sutampa su nurodytu. Jei jie jau yra kanale, ði komanda neiðspiria jø "
"(reikia OP statuso kanale)"

#: src/common/outbound.c:2461
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr "UNBAN <mask> [<mask>...] - nuima banà nurodytiems maskams"

#: src/common/outbound.c:2462
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR - iðvalo esamà tekstiná langà"

#: src/common/outbound.c:2463
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE - uþdaro esamà langà/kortelæ"

#: src/common/outbound.c:2466
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"CTCP <vartotojas> <þinutë> - nusiunèia CTCP þinutæ vartotojui. Daþniausios "
"þinutës yra VERSION ir USERINFO"

#: src/common/outbound.c:2468
msgid "COUNTRY <code>, finds a country code, eg: au = australia"
msgstr "COUNTRY <kodas> - randa ðalá pagal kodà, pvz.: lt = Lietuva"

#: src/common/outbound.c:2470
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE - iðeina ið aktyvaus kanalo ir iðkarta á já gráþtà"

#: src/common/outbound.c:2472
#, fuzzy
msgid ""
"\n"
"DCC GET <nick>                     - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file] - send a file to someone\n"
"DCC LIST                           - show DCC list\n"
"DCC CHAT <nick>                    - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <vartotojas>           - gauna siûlomà bylà\n"
"DCC SEND <vartotojas> <failas> - siunèia bylà kitam asmeniui\n"
"DCC LIST                       - parodo DCC sesijø sàraðà\n"
"DCC CHAT <vartotojas>          - pasiûlo kitam asmeniui DCC pokalbá\n"
"DCC CLOSE <tipas> <vatrtotojas> <failas>     pavyzdys:\n"
"                    /dcc close send jonas byla.tar.gz"

#: src/common/outbound.c:2482
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <vardas> - iðtrina mygtukà ið po vartotojo sàraðo"

#: src/common/outbound.c:2484
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"DEHOP <vartotojas> - panaikina kanalo pusiau-operatoriaus teises vartotojui "
"aktyviame kanale (reikia kanalo operatoriaus teisiø)"

#: src/common/outbound.c:2486
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEOP <vartotojas> - panaikina kanalo operatoriaus teises vartotojui "
"aktyviame kanale (reikia kanalo operatoriaus teisiø)"

#: src/common/outbound.c:2488
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEVOICE <vartotojas> - panaikina balso teisæ vartotojui aktyviame kanale "
"(reikia kanalo operatoriaus teisiø)"

#: src/common/outbound.c:2489
msgid "DISCON, Disconnects from server"
msgstr "DISCON - atjungia nuo serverio"

#: src/common/outbound.c:2490
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <varototojas|hostas|ip> - randa vartotojo IP adresà"

#: src/common/outbound.c:2491
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <tekstas> - lokaliai atspausdina tekstà"

#: src/common/outbound.c:2494
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"EXEC [-o] <komanda> - ávykdo komandà. Tuo atveju, jei nurodomas -o "
"parametras, komandos gràþinamas tekstas bus nusiøstas á aktyvø kanalà, kitu "
"atveju jis nusiunèiamas á aktyvø langà."

#: src/common/outbound.c:2496
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"EXECKILL [-9] - nutraukia aktyvioje sesijoje vykdomà komandà. Jei nurodomas "
"parametras -9, procesui bus nusiøstas SIGKILL signalas"

#: src/common/outbound.c:2498
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP - nusiunèia procesui SIGSTOP signalà"

#: src/common/outbound.c:2499
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT - nusiunèia procesui SIGCONT signalà"

#: src/common/outbound.c:2500
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE - nusiunèia duomenis proceso stdin árenginiui"

#: src/common/outbound.c:2504
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHHQ - panaikina aktyvaus serverio siuntimo laukianèiø duomenø eilæ"

#: src/common/outbound.c:2506
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr ""
"GATE <hostas> [<portas>] - jungiasi per proxy serverá. Nutylimas portas - 23"

#: src/common/outbound.c:2511
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr ""
"HOP <vartotojas> - suteikia vartotojui kanalo pusiau-operatoriaus teises "
"(reikia kanalo operatoriaus teisiø)"

#: src/common/outbound.c:2513
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <mask> <tipai..> <parametrai..>\n"
"    mask - maskas, kuris bus ignoruojamas, pvz: *!*@*.takas.lt\n"
"    tipai - ignoruotini duomenø tipai, vienas ar keli ið ðiø:\n"
"            PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    parametrai - NOSAVE, QUIET"

#: src/common/outbound.c:2520
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"INVITE <vartotojas> [<kanalas>] - pakvieèia varotojà á kanalà. Nutylimasis - "
"aktyvus kanalas (reikia kanalo operatoriaus teisiø)"

#: src/common/outbound.c:2521
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <kanalas> - prisijungia prie kanalo"

#: src/common/outbound.c:2523
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"KICK <vartotojas> - iðspiria varotojà ið aktyvaus kanalo (reikia kanalo "
"operatoriaus teisiø)"

#: src/common/outbound.c:2525
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"KICKBAN <vartotojas> - iðbanina ir iðspiria vartotojà ið aktyvaus kanalo "
"(reikia kanalo operatoriaus teisiø)"

#: src/common/outbound.c:2527
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK - iððaukia naujà uþlaikymo patikrinimà"

#: src/common/outbound.c:2529
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <tekstas> - ieðko duotojo teksto buferyje"

#: src/common/outbound.c:2531
msgid "LOAD <file>, loads a plugin or script"
msgstr "LOAD <failas> - ákrauna áskiepá ar skriptà..."

#: src/common/outbound.c:2534
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr ""
"MDEHOP - atima kanalo pusiau-operatoriaus teises ið visø aktyviame kanale "
"jas turinèiø (reikia kanalo operatoriaus teisiø)"

#: src/common/outbound.c:2536
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr ""
"MDEOP - atima kanalo operatoriaus teises ið visø aktyviame kanale jas "
"turinèiø (reikia kanalo operatoriaus teisiø)"

#: src/common/outbound.c:2538
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr ""
"MKICK - iðspiria ið aktyvaus kanalo visus iðskyrus tave (reikia kanalo "
"operatoriaus teisiø)"

#: src/common/outbound.c:2540
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"ME <veiksmas> - nusiunèia veiksmà á aktyvø kanalà (veiksmai raðomi treèiuoju "
"asmeniu, pvz., /me paðoka)"

#: src/common/outbound.c:2541
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <vartotojas> <þinutë> - nusiunèia privaèià þinutæ"

#: src/common/outbound.c:2544
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES - iðvardina visus vartotojus esamame kanale"

#: src/common/outbound.c:2546
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <vartotojas> <þinutë> - nuciunèia vartotojui CTCP pastebëjimà"

#: src/common/outbound.c:2547
msgid "NEWSERVER <hostname> [<port>]"
msgstr "NEWSERVER <serveris> [<portas>]"

#: src/common/outbound.c:2548
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <vardas> - pakeicèia tavo vartotojo vardà"

#: src/common/outbound.c:2551
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"NOTICE <vartotojas/kanalas> <þinutë> - nusiunèia pastabà. Pastabos - tai "
"þinutës, á kurias turëtø bûti reaguojama automatiðkai"

#: src/common/outbound.c:2553
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr ""
"NOTIFY [<vartotojas>] - iðvardina tavo stebimø vartotojø sàraðà arba prideda/"
"iðbraukia ið jo vartotojà"

#: src/common/outbound.c:2555
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr ""
"OP <vartotojas> - suteikia vartotojui kanalo operatoriaus teises (reikia jas "
"turëti paèiam)"

#: src/common/outbound.c:2557
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr ""
"PART [<kanalas>] [<prieþastis>] - palieka kanalà, pagal nutylëjimà - aktyvøjá"

#: src/common/outbound.c:2559
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr ""
"PING <vartotojas | kanalas> - nusiunèia vartotojui arba kanalui CTCP PING "
"signalà"

#: src/common/outbound.c:2561
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr "QUERY <vartotojas> - atidaro privaèiø þinuèiø su vartotoju langà"

#: src/common/outbound.c:2563
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<prieþastis>] - atsijungia nuo aktyvaus serverio"

#: src/common/outbound.c:2565
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <tekstas> - nusiunèia neformatuotà tekstà serveriui"

#: src/common/outbound.c:2568
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"RECONNECT [-ssl] [<serveris>] [<portas>] [<slaptaþodis>] - gali bûti "
"iðkvieèiamas paprasèiausiai komandos /RECONNECT pagalba, kad ið naujo "
"prisijungti prie aktyvaus serverio arba /RECONNECT ALL, kad ið naujo "
"prisijungti prie visø atvirø serveriø"

#: src/common/outbound.c:2571
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"RECONNECT [<serveris>] [<portas>] [<slaptaþodis>] - gali bûti iðkvieèiamas "
"paprasèiausiai komandos /RECONNECT pagalba, kad ið naujo prisijungti prie "
"aktyvaus serverio arba /RECONNECT ALL, kad ið naujo prisijungti prie visø "
"atvirø serveriø"

#: src/common/outbound.c:2573
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"RECV <tekstas> - nusiunèia neformatuotà tekstà xchat programai, lyg ji já "
"bûtø gavusi ið irc serverio"

#: src/common/outbound.c:2576
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <tekstas> - nusiunèia tekstà objektui aktyviame lange"

#: src/common/outbound.c:2577
msgid "SET <variable> [<value>]"
msgstr "SET <kintamasis> [<reikðmë>]"

#: src/common/outbound.c:2581
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr ""
"SERVCHAN [-ssl] <serveris> <portas> <kanalas> - prisijungia ir áeina á kanalà"

#: src/common/outbound.c:2584
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN <serveris> <portas> <kanalas> - prisijungia ir áeina á kanalà"

#: src/common/outbound.c:2588
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr ""
"SERVER [-ssl] <serveris> [<portas>] [<slaptaþodis>] - prisijungia prie "
"serverio, nutylimasis portas yra 6667 paprastiems prisijungimams ir 9999 - "
"prisijungimams su ssl"

#: src/common/outbound.c:2591
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr ""
"SERVER <serveris> [<portas>] [<slaptaþodis>] - prisijungia prie serverio, "
"nutylimasis portas yra 6667"

#: src/common/outbound.c:2594
msgid ""
"TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr ""
"TOPIC [<tema>] - nustato kanalo temà, jei ji duota, arba parodo esamà temà"

#: src/common/outbound.c:2595
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <mask> [QUIET]"

#: src/common/outbound.c:2596
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "UNLOAD <vardas> - iðkrauna áskiepá ar skriptà"

#: src/common/outbound.c:2599
msgid ""
"WALLCHOP <message>, sends the message to all chanops on the current channel"
msgstr ""
"WALLCHOP <þinutë> - nusiunèia þinutæ visiems aktyvaus kanalo operatoriams"

#: src/common/outbound.c:2601
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <þinutë> - nusiunèia þinutæ visiems kanalams"

#: src/common/outbound.c:2603
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr ""
"VOICE <vartotojas> - suteikia vartotojuio balso teisæ (reikia kanalo "
"operatoriaus teisiø)"

